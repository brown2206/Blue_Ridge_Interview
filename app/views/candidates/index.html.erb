<h1>All Candidates</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>FIRST NAME</th>
      <th>LAST NAME</th>
      <th>EMAIL</th>
      <th>GENDER</th>
      <th>IP ADDRESS</th>
      <th>DATE OF BIRTH</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @candidates.each do |candidate| %>
      <tr>
        <td><%= candidate.id %></td>
        <td><%= candidate.first_name %></td>
        <td><%= candidate.last_name %></td>
        <td><%= candidate.email %></td>
        <td><%= candidate.gender %></td>
        <td><%= candidate.ip_address %></td>
        <td><%= candidate.date_of_birth %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
